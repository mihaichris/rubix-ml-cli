{
    "name": "mihaichris/rubix-ml-cli",
    "description": "The Rubix ML Console Application",
    "keywords": [
        "rubix",
        "ml",
        "cli"
    ],
    "homepage": "https://github.com/mihaichris/rubix-ml-cli",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "Mihai-Cristian Fagadau",
            "email": "contact@mihaifagadau.dev"
        }
    ],
    "require": {
        "php": "^8.1",
        "laminas/laminas-text": "^2.10",
        "laravel-zero/framework": "^10.0.2",
        "nunomaduro/termwind": "^1.15.1",
        "rubix/ml": "^2.4"
    },
    "require-dev": {
        "laravel/pint": "^1.8",
        "mockery/mockery": "^1.5.1",
        "pestphp/pest": "^2.5"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "bin": [
        "rubix-ml-cli"
    ]
}
